<header class="m-legend__header">
  <h2 class="title">Legend</h2>
  <span class="btn-minimize"></span>
</header>
<div class="m-legend__content">
  <ul class="m-legend__list">
  {{#each legends}}
    <li class="drag-items" data-id="{{this.id}}">
      <header>
        <h3>{{this.name}}</h3>
        <div class="actions"></div>
      </header>
      {{#if this.tpl}}
        {{{this.tpl}}}
      {{/if}}
    </li>
  {{/each}}
  </ul>
</div>
