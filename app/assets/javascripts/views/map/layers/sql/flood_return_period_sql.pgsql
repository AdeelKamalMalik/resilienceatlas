with geom as ( select st_memunion(the_geom_webmercator) as the_geom_webmercator from grpcountries_250k_polygon where region = 'Horn') select ST_SetBandNoDataValue(ST_SetBandNoDataValue(ST_ColorMap(st_union(st_clip(t.the_raster_webmercator,1,geom.the_geom_webmercator,NULL,TRUE)),'bluered'),1,-10),1,NULL) the_raster_webmercator from multreturnperiod_rcp85_20c100yr1 t, geom where st_intersects(geom.the_geom_webmercator, t.the_raster_webmercator)